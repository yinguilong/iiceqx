@model List<string>
@using iiceqx.Model;
@if (Model != null && Model.Any())
{
    var pageIndex = (int)ViewData["pageIndex"];
    for (int i = 0; i < Model.Count; i++)
    {
        <div class="col-lg-12">
            <h2>@Model[i]</h2>
            <p><a class="btn btn-primary" href="/article/Article/articlestudybytheme?themeName=@(System.Web.HttpUtility.UrlEncode(Model[i]))" role="button">学习该系列»</a></p>
        </div>
    }
    <nav>
        <ul class="pager">
            @if (pageIndex != 1)
            {
                <li><a href="javascript:void(0)" onclick="PageChaged(@(pageIndex-1))">Previous</a></li>
            }
            <li><a href="javascript:void(0)" onclick="PageChaged(@(pageIndex+1))">Next</a></li>
        </ul>
    </nav>
}